class Solution {
public:
    vector<int> dailyTemperatures(vector<int>& temp) {
        stack<int>st;
        vector<int>v(temp.size(),0);
      
        for(int i=temp.size()-1;i>=0;i--){
            while(!st.empty() && temp[st.top()]<=temp[i]){
                st.pop();
              
            }
           if(!st.empty()){
            v[i]=st.top()-i;
           }
           st.push(i);
        }
        return v;
    }
};