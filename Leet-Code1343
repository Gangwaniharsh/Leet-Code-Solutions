class Solution {
public:
    int numOfSubarrays(vector<int>& arr, int k, int threshold) {
        int sum=0,t,count=0;
        for(int i=0;i<arr.size();i++){
            sum=sum+arr[i];
            if(i>=k) sum=sum-arr[i-k];

            if(i>=k-1) {
                t=sum/k;
                if(t>=threshold){
                    count++;
                }
            }
        }
        return count;
    }
};