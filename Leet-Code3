class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        vector<int>freq(256,0);
        int left=0,ans=0;
        if(s.empty()){
            return ans;
        }
        for(int right=0;right<s.length();right++){
            freq[s[right]]++;
            while(freq[s[right]]>1){
                
                freq[s[left]]--;
                left++;

            }
            ans=max(ans,right-left+1);
        }
        return ans;
    }
};