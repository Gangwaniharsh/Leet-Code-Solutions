class Solution {
public:
    bool containsNearbyDuplicate(vector<int>& nums, int k) {
        unordered_set<int>s;
        int left=0;
        for(int right=0;right<nums.size();right++){
            if(s.count(nums[right])){
                return true;
            }

            s.insert(nums[right]);

            if(s.size()>k){
                s.erase(nums[left]);
                left++;
            }
        }
        return false;
    }
};