class Solution {
public:
    int largestAltitude(vector<int>& gain) {
        vector<int>pref(gain.size()+1,0);
        int ans=0;
        pref[0]=0;
        for(int i=0;i<gain.size();i++){
            pref[i+1]=pref[i]+gain[i];
            ans=max(ans,pref[i+1]);
        }
        return ans;
    }
};